    "def R(m:'Hiperplanos',n:'Numero de Dimensiones'):\n",
    "    if m == 0:\n",
    "        return 1\n",
    "    elif n == 1:\n",
    "        return m + 1\n",
    "    else:\n",
    "        return R(m-1,n)+R(m-1,n-1)"
   ]
